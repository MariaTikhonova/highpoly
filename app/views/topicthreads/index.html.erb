<div class="center jumbotron">
<h1>3d Modeller News Forum</h1>
<p>Create your topics and answer to authors</p>


  <%= link_to "Home", home_path, class: "btn btn-lg btn-primary" %>
</div>


<p id="notice"><%= notice %></p>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
 <thead style= "background-color: rgba(255, 255, 255, 0)" >
   <% @topicthreads.each do |topicthread| %>
  <thead><%= link_to topicthread.name, topicthread_path(topicthread) %></thead>
   <tbody>
  <table width="100%" border="0" cellspacing="0" cellpadding="0">
   <thead style= "background-color: #379392" >
      <tr>
      <th>Branch</th>
      <th>Last topic</th>
      <th>By</th>
      <th>At</th>
      <th>Messages</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  <% render partial: "branchlist" %>
   </tbody>
   <% end %>
</table>


<br>



<%= link_to 'New Thread', new_topicthread_path %>