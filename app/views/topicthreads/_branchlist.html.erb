<div class="center jumbotron">


<p id="notice"><%= notice %></p>


  <table width="100%" border="0" cellspacing="0" cellpadding="0">
  <thead>
    <tr>
      <th>Branches</th>
      <th>Authors</th>
      <th>Messages</th>
      <th>Last message</th>
      <th>By</th>
      <th>At </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @branches.each do |branch| %>
       <tr>
        <td><%= link_to branch.name, branch_path(branch) %></td>
      
         <% end %>   
      </tr>
   
  </tbody>
</table>

